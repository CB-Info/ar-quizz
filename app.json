{
  "expo": {
    "name": "AR Quiz Langues",
    "slug": "ar-quiz-langues",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "arquiz",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera for AR functionality.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.bllfngr.arquizlangues"
    },
    "android": {
      "permissions": [
        "CAMERA",
        "CAMERA"
      ],
      "package": "com.bllfngr.arquizlangues"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "@reactvision/react-viro"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "52b6c15b-080d-4031-b8f4-fd794adbbea7"
      }
    }
  }
}
